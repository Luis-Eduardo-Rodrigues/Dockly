generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id_user     Int       @id @default(autoincrement())
  email_user  String    @unique
  nome_user   String
  senha_user  String
  qtd_tickets Int       @default(3)
  histories   History[]
}

model History {
  id        Int      @id @default(autoincrement())
  prompt    String  
  yml       String  
  createdAt DateTime @default(now())

  userId    Int      
  user      User     @relation(fields: [userId], references: [id_user])
}
